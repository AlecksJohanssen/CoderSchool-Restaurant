<!DOCTYPE html>
<html>
  <head>
    <title>CoderschoolRestaurant</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
<div class="container">
  <nav class="navbar navbar-light bg-faded">
    <%= link_to "CoderschoolRestaurant", root_path, class: 'navbar-brand'%>
      <ul class="nav navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href=<%= menu_path %>>Menu<span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href=<%= contact_us_path%>>Contact Us</a>
        </li>
      </ul>
    <form class="form-inline pull-xs-right">
      <input class="form-control" type="text" placeholder="Search">
      <button class="btn btn-success-outline" type="submit">Search</button>
    </form>
  </nav>
</div>
    <div class="container">
       <%= yield %>
    </div>

  </body>
</html>
